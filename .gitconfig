[include]
	path = ~/.gitconfig.local
[alias]
	co = checkout
	done = !git fetch && git rebase origin/master && git checkout master && git merge @{-1} --ff-only && git push
	ir = !git rebase -i origin/master
	up = !git fetch origin && git rebase origin/master
[commit]
	template = ~/.gitmessage
[color]
	ui = true
[push]
	default = current
[core]
	editor = subl
